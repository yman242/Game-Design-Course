    {
        image_index = idleMin;
    }
}
else if(!destroying)
{
    if(image_index&lt;moveStart)
    {
        image_index = moveStart;
    }
    if(image_index&gt;=moveMax)
    {
        image_index = moveMin;
    }
}
else
{  
